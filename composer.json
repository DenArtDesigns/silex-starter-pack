        ,"silex/silex": "~2.0"
        ,"symfony/twig-bridge": "3.2.*"
        ,"symfony/translation": "3.2.*"
        ,"symfony/config": "3.2.*"
        ,"symfony/yaml": "3.2.*"
        ,"symfony/form": "3.2.*"
        ,"symfony/validator": "3.2.*"
        ,"symfony/console": "3.2.*"
        ,"symfony/security": "3.2.*"
        ,"doctrine/dbal": "^2.5"
        ,"twig/twig": ">=1.8,<2.0-dev"
    }
    ,"autoload": {
        "psr-0": {
            "DC\\SilexStarterPack": "src/"
        }
    }
}
