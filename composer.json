{
    "name": "deanc/silex-starter-pack"
    ,"type": "library"
    ,"description": "A boilerplate to get started with silex"
    ,"keywords": ["silex", "symfony", "boilerplate", "php"]
    ,"homepage": "https://github.com/deanc/silex-starter-pack"
    ,"license": "MIT"
    ,"authors": [
        {
            "name": "Dean Clatworthy"
            ,"homepage": "http://deanclatworthy.com"
        }
    ],
    "require": {
        "php": ">5.3.2"
        ,"silex/silex": "1.*"
        ,"twig/twig": ">=1.8,<2.0-dev"
        ,"symfony/twig-bridge": "2.6.*"
        ,"symfony/translation": "2.6.*"
        ,"symfony/config": "2.6.*"
        ,"symfony/yaml": "2.6.*"
        ,"doctrine/dbal": "2.5.*"
        ,"symfony/form": "2.6.*"
        ,"symfony/validator": "2.6.*"
        ,"symfony/console": "2.6.*"
        ,"symfony/security": "2.6.*"
    }
    ,"autoload": {
        "psr-0": {
            "DC\\SilexStarterPack": "src/"
        }
    }
}
