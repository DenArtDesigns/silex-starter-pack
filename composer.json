{
    "name": "deanc/silex-starter-pack"
    ,"type": "library"
    ,"description": "A boilerplate to get started with silex"
    ,"keywords": ["silex", "symfony", "boilerplate", "php"]
    ,"homepage": "https://github.com/deanc/silex-starter-pack"
    ,"license": "MIT"
    ,"authors": [
        {
            "name": "Dean Clatworthy"
            ,"homepage": "http://deanclatworthy.com"
        }
    ],
    "require": {
        "php": ">5.3.2"
        ,"silex/silex": "1.*"
        ,"twig/twig": ">=1.8,<2.0-dev"
        ,"symfony/twig-bridge": "~2.3"
        ,"symfony/translation": "~2.3"
        ,"symfony/config": "~2.3"
        ,"symfony/yaml": "~2.3"
        ,"doctrine/dbal": "2.2.*"
        ,"symfony/form": "~2.3"
        ,"symfony/validator": "~2.3"
        ,"symfony/console": "~2.3"
        ,"symfony/security": "~2.3"
        ,"ircmaxell/password-compat": "dev-master"
    }
    ,"autoload": {
        "psr-0": {
            "DC\\SilexStarterPack": "src/"
        }
    }
}
